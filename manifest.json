{
  "manifest_version": 2,
  "name": "Scentrealm Video controller",
  "description": "This extension control the Scentrealm device play smell when play videos",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.iqiyi.com/*",
        "https://www.iqiyi.com/*",
        "http://v.youku.com/*",
        "https://v.youku.com/*"
      ],
      "js": [
        "lib/require.js",
        "lib/jquery.min.js",
        "build/sdk.min.js",
        "content.js"
      ],
      "css":[
        "style.css"
      ]
    }
  ]
}